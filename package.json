{
  "name": "skylive",
  "productName": "SkyLive",
  "main": "electron/main.js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "cross-spawn": "^7.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "tree-kill": "^1.2.2",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "start-win": "set \"BROWSER=none\" && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000  && electron .",
    "start-electron-win": "set \"ELECTRON_START_URL=http://localhost:3000\"  && electron .",
    "build-electron": "mkdir build/src && cp -r electron/. build/electron && cp -r src/shared/. build/src/shared && cp commands.js build/",
    "package": "electron-builder build --linux --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-linux": "electron-builder build --linux -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-win": " electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never",
    "deploy": "electron-builder build --linux --win -c.extraMetadata.main=build/electron/main.js --publish always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0"
  },
  "homepage": "./"
}
